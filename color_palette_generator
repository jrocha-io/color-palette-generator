rtHue, contrastAndDeltaList):\n","    all_palettes = {}\n","    \n","    # Generate a grid of all possible HSL combinations once\n","    hsl_values = [(h, s, l) for h in range(startHue, startHue + 360) for s in range(100, 1, -1) for l in range(1, 99)]\n","    \n","    for requiredContrastRatio, deltaEThreshold in contrastAndDeltaList:\n","        print(\"----------\")\n","        print(\"Contrast: \" + str(requiredContrastRatio))\n","        print(\"----------\")\n","        \n","        # Filter HSL values by approximate contrast ratio\n","        filtered_hsl = [\n","            hsl for hsl in hsl_values\n","            if math.isclose(contrast_ratio(hsl_to_rgb(*hsl)), requiredContrastRatio, rel_tol=1e-2)\n","        ]\n","        \n","        # Convert HSL to RGB and apply Delta E filtering\n","        colors = []\n","        for h, s, l in filtered_hsl:\n","            rgb = hsl_to_rgb(h, s, l)\n","            if all(delta_e(rgb, existing) >= deltaEThreshold for existing in colors):\n","                colors.append(rgb)\n","        \n","        # Sort colors by Chroma in descending order\n","        colors = sorted(colors, key=chroma, reverse=True)\n","        \n","        # Store palette\n","        all_palettes[f\"Contrast {requiredContrastRatio}\"] = [\"#{:02X}{:02X}{:02X}\".format(*c) for c in colors]\n","        print(all_palettes[f\"Contrast {requiredContrastRatio}\"])\n","\n","    return all_palettes"]},{"cell_type":"code","execution_count":9,"id":"02360421","metadata":{"execution":{"iopub.execute_input":"2025-01-28T21:02:52.954818Z","iopub.status.busy":"2025-01-28T21:02:52.954428Z","iopub.status.idle":"2025-01-28T22:03:21.038901Z","shell.execute_reply":"2025-01-28T22:03:21.037651Z"},"papermill":{"duration":3628.099615,"end_time":"2025-01-28T22:03:21.047523","exception":false,"start_time":"2025-01-28T21:02:52.947908","status":"completed"},"tags":[]},"outputs":[{"name":"stdout","output_type":"stream","text":["=====================================\n","Gene